body {margin:0px; padding:0px; font-family:Verdana, Arial; font-size:12px}

img {border:0px; display:inline;  vertical-align:middle}
.clear {clear:both; font-size:1px; line-height:1px}

h3 {margin:5px; padding:0px; font-size:18px; font-weight:normal}
h1{margin:5px; padding:10px 0px; font-size:26px; font-weight:normal}
form {margin:0px; padding:0px; display:inline}
input.input, textarea.input {border:2px solid #ccc; padding:3px}

input.submit {border:1px solid transparent; padding:3px}

.continut {width:1000px; margin:auto; overflow:visible}
.box_mesaj {border:2px solid #000; padding:5px; font-size:14px; color:#333; font-weight:bold; margin:10px; display:block}

.info_profil {margin-left:30px}
p.titlu {font-weight:bold; font-size:14px}
p.text {padding:0px; margin:5px 0px}
p.text_i {text-indent:30px}



#header {width:1000px; height:170px; margin:auto; overflow:auto; margin-bottom:20px}
#header #logo {width:200px; height:170px; float:left}
#header #banner {width:780px; height:110px; padding:10px; float:left; position:relative}
#header #banner #meniu {position:absolute; bottom:5px; left:0px; height:30px; line-height:30px; font-size:16px; font-weight:bold}
#header #banner #meniu img {margin-left:10px}
#header #banner #meniu a {color:#000000; margin:0px}
#header #banner #meniu a:hover {color:#002245}
#header #banner #meniu2 {float:right}
#header #scroll {width:800px; height:26px; float:left; border-top:2px solid #000; border-bottom:2px solid #000; line-height:24px}


#meniu_prima{float:left; width:125px; height:94px; line-height:94px; display:block; background:#345345 url('../images/meniu_prima.jpg') top left no-repeat}
#meniu_prima:hover{background:#fff url('../images/meniu_prima.jpg') top left no-repeat}
#meniu_barfe{float:left; width:91px; height:94px; line-height:94px; display:block; background:#345345 url('../images/meniu_barfe.jpg') top left no-repeat}
#meniu_barfe:hover{background:#fff url('../images/meniu_barfe_on.jpg') top left no-repeat}
#meniu_profil{float:left; width:96px; height:94px; line-height:94px; display:block; background:#345345 url('../images/meniu_profil.jpg') top left no-repeat}
#meniu_profil:hover{background:#fff url('../images/meniu_profil_on.jpg') top left no-repeat}
#meniu_prieteni{float:left; width:100px; height:94px; line-height:94px; display:block; background:#345345 url('../images/meniu_prieteni.jpg') top left no-repeat}
#meniu_prieteni:hover{background:#fff url('../images/meniu_prieteni_on.jpg') top left no-repeat}




#left {width:180px; float:left; text-align:center;}
#middle {width:550px; float:left; margin:0px 10px}
#middle2 {width:800px; float:left; margin:0px 10px}
#right {width:250px; float:left; text-align:center}
#right p{font-size:16px; line-height:22px}

#footer {width:1000px; height:30px; margin:auto; margin-top:20px; margin-bottom:10px; line-height:30px}
#footer img{display:middle}
#footer a{font-size:14px; color:#333333}

#connect_fb{width:254px; height:99px; line-height:254px; display:block; background:#345345 url('../images/connect_fb.jpg') top left no-repeat}
#connect_fb:hover{background:#345345 url('../images/connect_fb2.jpg') top left no-repeat}




/*  linkuri stanga dreapta */
.img_profil {margin:10px 5px; float:left}
.a_barfa {line-height:20px; color:#000; width:100%; display:block; margin:4px 0px; text-align:left}
.a_barfa_on {color:#002145;}
.a_barfa img {margin-right:5px}

.a_text{line-height:24px; color:#000; margin:4px 0px; font-weight:bold; font-size:14px}
.a_text_on{color:#002145; font-weight:bold; font-size:14px}
.a_text img {margin-right:5px; }

.poza_profil {width:170px}

.barfa1 {width:550px; padding:10px 0px; margin:15px 0px; border-top:1px solid #cccccc; border-bottom:1px solid #cccccc; overflow:auto}
.barfa1 .poza_left, .barfa2 .poza_left  {width:60px; float:left;}
.barfa1 .info_right {width:490px; float:left}
.barfa1 .titlu_barfa, .barfa2 .titlu_barfa {width:100%; float:left}
.barfa1 .titlu_barfa  .intro, .barfa2 .titlu_barfa  .intro {font-size:12px; margin:0px; padding:3px}
.barfa1 .titlu_barfa  .titlu, .barfa2 .titlu_barfa  .titlu {font-size:22px; color:#002145;  margin:0px; padding:3px}
.barfa1 .text_barfa1 {float:right; width:320px; font-size:12px; padding:10px; text-align:justify}
.barfa1 .text_barfa2 {float:right; width:470px; font-size:12px; padding:10px; text-align:justify}
.barfa1 .poze_barfa , .barfa2 .poze_barfa {width:150px; float:right; text-align:center}
.barfa1 .poze_barfa img, .barfa2.poze_barfa img {width:140px; margin:5px}
.barfa1 .poze_barfa a, .barfa1 .poze_barfa a {font-size:12px; font-weight:bold; color:#002145}

.barfa1 .tools_barfa {width:490px; float:left}
.barfa1 .tools_barfa  .tools_left {width:120px; float:left; margin-right:10px}
.barfa1 .tools_barfa  .tools_right {width:360px; float:left}
.barfa1 .tools_barfa p {padding:0px; margin:0px; line-height:20px; font-size:10px}
.barfa1 .tools_barfa span {margin:0px 5px}
.barfa1 .tools_barfa a {margin:0px 5px; color:#666666}

.barfa1 .comm {width:490px; float:left; margin:5px 0px}
.barfa1 .comm .comm_poza {width:60px; float:left}
.barfa1 .comm .comm_text {width:398px; padding:5px; margin-left:10px; float:left; border:1px solid #ccc; -moz-border-radius:5px; -webkit-border-radius:5px}
.barfa1 .comm .comm_text p.text{font-size:12px; padding:2px 0px; margin:0px}
.barfa1 .comm .comm_text p.text a {color:#002145; margin-right:10px}
.barfa1 .comm .comm_text p.info{font-size:10px; padding:2px 0px; margin:0px}


.barfa2 {width:800px; padding:10px 0px; margin:15px 0px; border-top:1px solid #cccccc; border-bottom:1px solid #cccccc; overflow:auto}
.barfa2 .info_right {width:740px; float:left}
.barfa2 .text_barfa1 {float:right; width:570px; font-size:12px; padding:10px; text-align:justify}
.barfa2 .text_barfa2 {float:right; width:720px; font-size:12px; padding:10px; text-align:justify}

.friend {width:550px; margin:10px 0px; overflow:auto}
.friend .img_profil {margin:10px 5px; float:left; width:50px}
.friend .name {width:490px; font-size:24px; float:left; margin:2px 0px}
.friend .tools {width:490px; float:left;  margin:2px 0px}