{% load prettyuser %}
{% for friend in friends %}
<div class="friend"><div>
<img src="{{friend.get_profile.photo.thumbnail.url}}" width="50"/>
<a href="{% url profile-view username=friend.username %}">{{friend|user_or_name}}</a>
</div>
</div>
{% endfor %}

<div class="nav-friends">
{% if has_prev %}
<a href="#" onclick="get_friends({'page': friends_page-=1});">inapoi </a>
{% endif %}
{% if has_next %}
<a href="#" onclick="get_friends({'page': friends_page+=1});">mai departe </a>
{% endif %}
</div>